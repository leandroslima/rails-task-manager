<h1>New Task</h1>

<%= form_with model: @task do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :details %>
  <%= form.text_area :details %>
  <%= form.label :completed %>
  <%= form.text_area :completed %>
  <%= form.submit %>
<% end %>

